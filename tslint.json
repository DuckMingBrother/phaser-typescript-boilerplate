{
  "extends": "tslint:recommended",
  "rules": {
    "arrow-parens": false,
    "interface-name": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "max-line-length": [
      true,
      255
    ],
    "member-access": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      "double"
    ],
    "space-before-function-paren": false,
    "trailing-comma": [
      false,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-empty": false,
    "no-namespace": false,
    "no-console": [
      "debug"
    ],
    "ban-types": false,
    "one-line": [
      "check-else"
    ],
    "no-empty-interface": false,
    "semicolon": [
      false
    ],
    "member-ordering": [
      false
    ],
    "variable-name": [
      "allow-leading-underscore"
    ],
    "no-unused-expression": false,
    "radix": false,
    "no-bitwise": false,
    "forin": false,
    "one-variable-per-declaration": [
      false
    ],
    "indent": [
      true,
      4
    ],
    "no-unused-variable": true
  }
}